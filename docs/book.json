{
  "title": "Stream Your Care",
  "description": "A guide for mitigating isolation, neglect, and abuse with livestream technology",
  "author": "Mundi",
  "language": "en",
  "root": "./",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md",
    "glossary": "GLOSSARY.md"
  },
  "plugins": [
    "toggle-chapters",
    "edit-link",
    "octocat",
    "-lunr",
    "copy-code-button",
    "sitemap"
],
  "pluginsConfig": {
    "toggle-chapters": {},
    "sitemap": {
      "url": "https://streamyourcare.com"
    },
    "edit-link": {
      "base": "https://github.com/streamyourcare/streamyourcare/edit/main/docs/",
      "label": "Edit This Page"

    },
    "octocat": {
      "url": "https://github.com/streamyourcare/streamyourcare"
    },
    "theme-default": {
      "showLevel": false
    }
  }
}
  